<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.ursurbornyar.mapper.PointSetMapper">
	
	<insert id = "insertPointSet" parameterType="com.my.ursurbornyar.vo.Path">
		INSERT INTO POINT_SET_TB (
			ID,
			START_POINT_ID,
			END_POINT_ID
		)
		VALUES (
			#{path.id},
			#{path.start_point_id},
			#{path.end_point_id}
		)
	</insert>
	
	<select id = "countPointSet" resultType = "Integer">
		SELECT
			COUNT(*)
		FROM
			POINT_SET_TB
	</select>
		
	
</mapper>